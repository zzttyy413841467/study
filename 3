#include "stdio.h"
void main()
{
	char c1 = 'C', c2 = 'h', c3 = 'i', c4 = 'n', c5 = 'a';
	c1 += 4;                    //计算对应的密码
	c2 += 4;
	c3 += 4;
	c4 += 4;
	c5 += 4;
	printf("密码是");
	putchar(c1);                //用putchar输出密码
	putchar(c2);
	putchar(c3);
	putchar(c4);
	putchar(c5);
	putchar('\n');
	printf("密码是%c%c%c%c%c\n", c1, c2, c3, c4, c5);//用printf输出密码
}
