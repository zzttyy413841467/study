#include "stdio.h"
#include "math.h"
#define PI 3.14159              //定义符号常量PI
void main()
{
	float r, h, c, s, S, V1, V2; 
	scanf_s("%f%f", &r, &h);          //接收输入
	c = 2 * PI*r;                    //计算
	s = PI*pow(r, 2);
	S = 4 * PI*pow(r, 2);
	V1 = 4 * PI*pow(r, 3) / 3;
	V2 = s*h;
	printf("圆周长为%.2f\n圆面积为%.2f\n圆球表面积为%.2f\n圆球体积为%.2f\n圆柱体积为%.2f\n", c, s, S, V1, V2);//输出结果
}
